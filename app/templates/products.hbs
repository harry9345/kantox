<GeneralContainer>
  <BreadCrumbs
    @pageTitle="Groceries"
    @pageSubtitle="Special FX offers only the finest organic produce."
    @breadCrumbs={{array "Groceries" "Wine" "Clothes" "FX"}}
  />
  <div class="flex flex-row gap-14">
    {{#each this.model as |product|}}
      <LinkTo @route="item" @model={{product.id}}>
        <Product @product={{product}} />
      </LinkTo>
    {{/each}}
  </div>
</GeneralContainer>